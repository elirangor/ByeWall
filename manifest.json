{
  "manifest_version": 3,
  "name": "ByeWall",
  "version": "1.7.6",
  "description": "Bypass paywalls quickly by accessing archived versions via Archive.today or Wayback Machine. View recently archived articles in a clean interface.",
  "action": {
    "default_title": "ByeWall",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "activeTab"],
  "host_permissions": [
    "https://archive.today/*",
    "https://archive.ph/*",
    "https://web.archive.org/*",
    "https://archive.org/*"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}