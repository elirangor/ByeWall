{
  "manifest_version": 3,
  "name": "ByeWall",
  "version": "1.7.3",
  "description": "Quickly open archived snapshots without opening empty tabs.",
  "action": {
    "default_title": "ByeWall",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://archive.today/*",
    "https://archive.ph/*",
    "https://web.archive.org/*",
    "https://archive.org/*"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
